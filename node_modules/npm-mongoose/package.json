  "description": "NPM Gobal Mongoose Library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:compile && npm run build:types",
    "build:compile": "npm run lint && babel src --extensions .ts --out-dir lib",
