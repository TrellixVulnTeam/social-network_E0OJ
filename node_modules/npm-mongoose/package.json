    "eslint": "^8.13.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "sinon": "^13.0.2",
    "sinon-chai": "^3.7.0",
